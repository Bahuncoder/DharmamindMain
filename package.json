{
  "name": "dharmamind-waitlist",
  "version": "1.0.0",
  "description": "Professional waitlist landing page for DharmaMind AI",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "echo 'Build complete - static site ready'",
    "test": "npm run test:lighthouse && npm run test:accessibility",
    "test:lighthouse": "lhci autorun",
    "test:accessibility": "axe http://localhost:8080",
    "optimize": "npm run minify:css && npm run minify:js",
    "minify:css": "cleancss -o style.min.css style.css",
    "minify:js": "terser script-enhanced-v2.js -o script.min.js",
    "serve": "node server.js",
    "serve:static": "npx http-server . -p 8080",
    "deploy:github": "npm run build && git add . && git commit -m 'Deploy updates' && git push origin main",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod",
    "check:seo": "npm run test:lighthouse -- --only-categories=seo",
    "check:performance": "npm run test:lighthouse -- --only-categories=performance",
    "validate:html": "html-validate index.html",
    "validate:css": "stylelint style.css",
    "validate:js": "eslint script.js server.js"
  },
  "keywords": [
    "ai",
    "dharma",
    "mindfulness",
    "waitlist",
    "landing-page",
    "seo",
    "conversion",
    "nodejs",
    "express",
    "serverless"
  ],
  "author": "DharmaMind AI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/dharmamind-waitlist.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/dharmamind-waitlist/issues"
  },
  "homepage": "https://dharmamindai.com",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "nodemailer": "^7.0.9"
  },
  "devDependencies": {
    "@axe-core/cli": "^4.8.2",
    "@lhci/cli": "^0.12.0",
    "autoprefixer": "^10.4.23",
    "clean-css-cli": "^5.6.2",
    "eslint": "^8.55.0",
    "html-validate": "^8.7.3",
    "http-server": "^14.1.1",
    "nodemon": "^3.0.2",
    "postcss": "^8.5.6",
    "stylelint": "^15.11.0",
    "tailwindcss": "^4.1.18",
    "terser": "^5.24.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
